-- globals for init

-- canvas tile size
f_size = 16
s_size = 8

-- scope coords
scope_x = 64
scope_y = 64

-- four studios logo
function startup()
 cls()
 rectfill(47, 60, 52, 65, 11)
 rectfill(47, 64, 48, 65, 0)
 delay(5)
 rectfill(47, 52, 52, 57, 11)
 rectfill(47, 52, 48, 53, 0)
 delay(5)
 rectfill(55, 52, 60, 57, 11)
 rectfill(59, 52, 60, 53, 0)
 delay(5)
 rectfill(55, 60, 60, 65, 11)
 rectfill(59, 64, 60, 65, 0)
 delay(5)
 rectfill(53, 58, 54, 59,11)

 delay(10)
 color(7)
 print("four", 63, 54)
 delay(10)
 color(6)
 print("studios", 63, 60)
 
 delay(30)
 cls()
   
end

function delay(t)
 for i=0, t do
  flip()
 end
end

function get_highscore()
 cartdata("sniper")
 highscore = dget(0)
end

function _init()
 
 get_highscore()
 -- intro logo
 startup()
 
 -- title screen
 scene = "title"
 
 -- iniitial enemies
 set_enemies()
 
end
