-- globals for enemies
ene_max = 0
ene_time = 2 * ene_max

ene_mod = 5

function set_enemies()

 -- matrix for enemies
 enemies = {}
 
 -- fill matrix
 for i = 0, 15 do
	  enemies[i] = {}
	  for j = 0, 15 do
	   enemies[i][j] = 0
	  end
	end
	
	-- fill matrix with 
 for i = 0, 15 do  
  for j = 0, 15 do
  
   -- randomizer
   ran = flr(rnd(50))
  
   if((i == 1 and 
       j == 13)) then
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
   elseif(i == 15 and 
          j == 12) then
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
   elseif((i >= 0 and 
           i <= 3) and
          (j >= 5 and 
           j <= 9)) then 
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
   elseif((i >= 5 and 
           i <= 8) and
          (j >= 1 and 
           j <= 4)) then 
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
   elseif((i >= 5 and 
           i <= 14) and
          (j >= 5 and 
           j <= 10)) then 
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
   elseif(i == 15 and
         (j >= 8) and 
          j <= 10) then 
    if(ran == 0 and
       ene_max < ene_mod) then
     enemies[i][j] = 32
     ene_max += 1
    end
	  else
	   enemies[i][j] = 0
	  end
  
  end -- j
 end -- i
 
 ene_time = 2 * ene_max

end

