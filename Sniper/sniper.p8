-- globals for draw
trg_acq = 3

function _draw()

 if(scene == "title") then
 	draw_title()
 elseif(scene == "instr") then
  draw_instr()
 elseif(scene == "game") then
  draw_game()
 elseif(scene == "end") then
 	draw_end()
 end
 
end

function draw_game()
 cls()
 --static map, can add more
 map() 
 -- draw enemies
 draw_enemies()
 -- draw scope
 draw_scope()
 -- draw timer
 draw_timer()
 -- draw score
 draw_score()
 -- draw bullets left
 draw_bullets()
end

function draw_title()

	cls()
	for i = 40, 20, -1 do
		rectfill(63, i, 63.5,80.5-i, 2)
		rectfill(i+23, 40, 103.5-i,40.5, 2)
		delay(0.5)
	end
	
	circ(63.25, 40.25, 18, 6)
	
	spr(65,43,70)
	delay(1)
	spr(66,51,70)
	delay(1)
	spr(67,57,70)
	delay(1)
	spr(68,62,70)
	delay(1) 
	spr(69,70,70)
	delay(1)
	spr(70,76,70)
	delay(3)  
	print("press ‚ùé to start", 30, 100, 7)
	scene = "title_idle"
	
end

