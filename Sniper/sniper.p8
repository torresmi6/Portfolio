end

function draw_end()
	cls()
	spr(71,23,20)
	delay(1)
	spr(72,32,20)
	delay(1)
	spr(73,41,20)
	delay(1)
	spr(74,50,20)
	delay(1) 
	spr(75,67,20)
	delay(1)
	spr(76,76,20)
	delay(1)
	spr(74,85,20)
	delay(1)
	spr(77,94,20)
	delay(3)
	print("highscore: "..highscore, 38, 50, 10)
	print("your score: "..score, 38, 60, 10)  
	print("press ‚ùé to retry", 30, 90, 7)
	print("press üÖæÔ∏è to return to title", 10, 110, 7)
	scene = "end_idle"
end

function draw_scope()
 -- draw middle crosshair
 line(scope_x-7, scope_y,
      scope_x+7, scope_y,
      trg_acq)
 line(scope_x, scope_y-7,
      scope_x, scope_y+7,
      trg_acq)     
 circ(scope_x, scope_y, 6,
      trg_acq) 
 circ(scope_x, scope_y, 5,
      trg_acq)    
end

function draw_enemies()
 
 for i = 0, 15 do
  for j = 0, 15 do
   spr(enemies[i][j],
       i*8, j*8)
  end
 end
end

function draw_timer()
 cursor(64, 0, 7)
 print(timer_sec)
end

function draw_score()
 cursor(108, 0, 7)
 print(10000+score)
 
 rectfill(108, 0, 110, 4, 0)
 
 cursor(88, 0, 7)
 print("score:")
end

function draw_bullets()
 for i = 0, bullets-1 do
  spr(64, i*8, 0)
 end
end
