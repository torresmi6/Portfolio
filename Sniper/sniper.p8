-- note: function update every
-- 60 fps

-- globals for update
scp_spd = 1
dia_spd = scp_spd*0.71

score = 0

bullets = 3

-- 5 second timer
timer = 10*60

function _update60()

 if(scene == "title" or
    scene == "title_idle") then
  if(scene == "title") then
   sfx(1)
  end
	 title_controls()
	elseif(scene == "instr") then
	 instr_controls()
	elseif(scene == "game") then
	 game_controls()
	elseif(scene == "end_idle") then
		end_controls()
	end

end

function title_controls()
	if btnp(‚ùé) then
		scene = "instr"
	end
end

function instr_controls()
	if btnp(‚ùé) then
		scene = "game"
		music(0)
	end
end

function end_controls()
	if btnp(‚ùé) then
		scene = "instr"
	elseif btnp(üÖæÔ∏è) then
		scene = "title"
		sfx(1)
	end
end

function game_controls()

 --set timer
	set_timer()
  
 -- change scope color if on
 -- target
 target()
 
 -- draw new enemies when
 -- all prev are cleared
 new_enemies()
 
 -- lose
 lose()

 if(btnp(üÖæÔ∏è,0)) then

 end
 if(btnp(‚ùé,0)) then
  sfx(0)
  if(trg_acq == 8) then
   enemies[flr(scope_x/8)][flr(scope_y/8)] = 0
   ene_max -= 1
   score += 1
  elseif(trg_acq == 3) then
   bullets -= 1
  end
 end
 
 -- diagonals
 if(btn(‚¨ÜÔ∏è,0) and
    btn(‚¨ÖÔ∏è,0)) then
  scope_y -= dia_spd
  scope_x -= dia_spd
  bound_chk()
 elseif(btn(‚¨ÜÔ∏è,0) and
        btn(‚û°Ô∏è,0)) then
  scope_y -= dia_spd
  scope_x += dia_spd
  bound_chk()
 elseif(btn(‚¨áÔ∏è,0) and
       btn(‚¨ÖÔ∏è,0)) then
  scope_y += dia_spd
  scope_x -= dia_spd 
  bound_chk() 
 elseif(btn(‚¨áÔ∏è,0) and     
        btn(‚û°Ô∏è,0)) then
  scope_y += dia_spd
  scope_x += dia_spd
  bound_chk()
 -- up
 elseif(btn(‚¨ÜÔ∏è,0)) then
   scope_y -= scp_spd
   bound_chk()
 -- down
 elseif(btn(‚¨áÔ∏è,0)) then
   scope_y += scp_spd
   bound_chk()
 -- left
 elseif(btn(‚¨ÖÔ∏è,0)) then
   scope_x -= scp_spd
   bound_chk()
 -- right
 elseif(btn(‚û°Ô∏è,0)) then
   scope_x += scp_spd
   bound_chk()
 end
 
end

function bound_chk()
 -- stay within boundary
 if(scope_x < 0) then
  scope_x = 0
 elseif(scope_x > 127) then
  scope_x = 127
 elseif(scope_y < 0) then
  scope_y = 0
 elseif(scope_y > 127) then
  scope_y = 127
 end
end

function target()
 local tar = (
       enemies[flr(scope_x/8)]
              [flr(scope_y/8)])
 if(tar == 16 or
    tar == 32 or
    tar == 48) then
  trg_acq = 8
 else
  trg_acq = 3
 end
end

function new_enemies()
 if(ene_max == 0) then
  if(score%50 == 0) then
   ene_mod += 1
  end
  set_enemies()
  timer = ene_time*60
  --bullets = 3
 end
end

function set_timer()
 timer -= 1
 timer_sec = ceil(timer/60)
end

function lose()
 if(bullets == 0 or
    timer == -1) then
  if(score >= highscore) then
   highscore = score
   dset(0, highscore)
  end
  dset(0, 0)
  scene = "end"
  music(6)
  sfx(2)
  reset_game()
 end
end

function reset_game()
  ene_max = 0
  ene_time = 2 * ene_max
  ene_mod = 5
  set_enemies()
  timer = 10*60
  bullets = 3
end
